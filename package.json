{
    "name": "email-verifier",
    "description": "Receive emails and verify them",
    "version": "0.0.1",
    "private": true,
    "devDependencies": {
        "@types/uuid": "^9.0.0",
        "jest": "^29.2.1"
    },
    "scripts": {
        "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js"
    },
    "jest": {
        "testMatch": [
            "**/__tests__/**/*.[jt]s?(x)",
            "**/?(*.)+(spec|test).[jt]s?(x)",
            "**/__tests__/**/*.mjs?(x)",
            "**/?(*.)+(spec|test).mjs?(x)"
        ],
        "moduleFileExtensions": [
            "mjs",
            "js"
        ]
    },
    "dependencies": {
        "aws-cdk-lib": "^2.53.0",
        "aws-sdk": "^2.1265.0",
        "save": "^2.9.0",
        "save-dev": "^0.0.1-security",
        "uuid": "^9.0.0"
    }
}
